<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ElPais Parallel Suite" parallel="tests" thread-count="5">
    <test name="Windows Chrome">
        <parameter name="browserConfig" value="bs1"/>
        <classes>
            <class name="com.elpais.runner.MainRunner"/>
        </classes>
    </test>
    <test name="Windows Firefox">
        <parameter name="browserConfig" value="bs2"/>
        <classes>
            <class name="com.elpais.runner.MainRunner"/>
        </classes>
    </test>
    <test name="Mac Safari">
        <parameter name="browserConfig" value="bs3"/>
        <classes>
            <class name="com.elpais.runner.MainRunner"/>
        </classes>
    </test>
    <test name="Android Chrome">
        <parameter name="browserConfig" value="bs4"/>
        <classes>
            <class name="com.elpais.runner.MainRunner"/>
        </classes>
    </test>
    <test name="iPhone Safari">
        <parameter name="browserConfig" value="bs5"/>
        <classes>
            <class name="com.elpais.runner.MainRunner"/>
        </classes>
    </test>
</suite>
